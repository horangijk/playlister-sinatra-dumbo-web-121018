<h1>This is <%= @artist.name %>'s page.</h1>

<ul>
  <%@artist.songs.each do |song|%>
    <li><a href = "/songs/<%=song.id%>"><%= song.name %></a>
      <% song.song_genres.each do |sg| %>
        <a href = "/genres/<%= sg.genre_id %>"><%= sg.genre.name %></a></li>
        <% end %>
  <% end %>
</ul>
